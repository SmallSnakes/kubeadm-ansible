image: {{ kube_image_repository }}/guest-service:{{ kube_image_tag }}
pullPolicy: "IfNotPresent"
pullSecrets: {{ docker_register_secrets_name }}

mqttVip: {{ kube_apiserver_vip }}
mqttPort: {{ emqx_mqtt_port }}
prometheus_url: http://{{ kube_apiserver_vip }}:{{ prometheus_pushgateway_port }}/metrics/job/pushgateway/instance/

serviceAccountName: guest-service
priorityClassName: {{ kube_priority_name }}

nodeSelector:
  {{ compute_node_role_name }}: {{ node_role_value }}

resources: {}
